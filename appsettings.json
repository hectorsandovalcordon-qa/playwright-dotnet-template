{
  "TestSettings": {
    "WebDriver": "playwright",
    "Browser": "chromium",
    "Headless": false,
    "SlowMo": 0,
    "Timeout": 30000,
    "ViewportWidth": 1920,
    "ViewportHeight": 1080,
    "Video": false,
    "Screenshot": "failure",
    "Trace": "retain-on-failure",
    "ImplicitWait": 10,
    "PageLoadTimeout": 30,
    "DriverPath": "",
    "DriverOptions": {
      "chrome": [
        "--disable-web-security",
        "--no-sandbox"
      ],
      "firefox": [
        "--disable-web-security"
      ],
      "edge": [
        "--disable-web-security",
        "--no-sandbox"
      ]
    }
  },
  "Environment": {
    "BaseUrl": "https://example.com",
    "ApiUrl": "https://api.example.com",
    "Username": "test@example.com",
    "Password": "password123"
  },
  "Logging": {
    "LogLevel": "Information",
    "LogToFile": true,
    "LogPath": "logs/"
  }
}
